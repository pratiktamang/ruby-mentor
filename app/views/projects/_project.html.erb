<div id="<%= dom_id project %>">
  <p>
    <strong>Title:</strong>
    <%= project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= project.status %>
  </p>

  <p>
    <strong>Mentee:</strong>
    <%= project.mentee_id %>
  </p>

  <p>
    <strong>Mentor:</strong>
    <%= project.mentor_id %>
  </p>

</div>
