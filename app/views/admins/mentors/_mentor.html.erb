<% if mentor.onboarded %>
  <tr>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.created_at.strftime('%B %d %Y') %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0">
      <span class="flex flex-col">
        <%= mentor.full_name %>
        <%= mentor.email %>
      </span>
    </td>
    <%# <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile.created_at.strftime('%B %d %Y') %1></td> %>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.company_url %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.ruby_start_year %></td>
    <td class="hidden px-3 py-4 text-sm text-gray-500 lg:table-cell"><%= mentor.mentor_profile&.country %></td>
    <td class="hidden px-3 py-4 text-sm text-gray-500 sm:table-cell"><%= mentor.mentor_profile&.city %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.twitter %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.github %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.personal_site %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.past_workplaces %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.previous_mentoring %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.motivation %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.learning_preference %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.availability %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.industry_expertise %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.mentor_profile&.specific_interests %></td>
    <td class="py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-0">
      <a href="#" class="text-indigo-600 hover:text-indigo-900">View</a>
    </td>
  </tr>
<% else %>
  <tr>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0"><%= mentor.created_at.strftime('%B %d %Y') %></td>
    <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0">
      <span class="flex flex-col">
        <%= mentor.full_name %>
        <%= mentor.email %>
      </span>
      <span class="inline-flex items-center rounded-md bg-red-50 px-2 py-1 text-xs font-medium text-red-700 ring-1 ring-inset ring-red-600/10">Not Onboarded</span>
    </td>
    <% 14.times do |i| %>
      <td class="w-full max-w-0 py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:w-auto sm:max-w-none sm:pl-0">-</td>
    <% end %>
    <td class="py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-0">
    </td>
  </tr>
<% end %>
